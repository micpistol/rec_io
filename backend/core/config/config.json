{
  "system": {
    "name": "Trading System",
    "version": "1.0.0",
    "environment": "development"
  },
  "agents": {
    "main": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 3000
    },
    "symbol_watchdog": {
      "enabled": true,
      "host": "0.0.0.0",
      "update_interval": 1.0,
      "providers": [
        "coinbase"
      ]
    },
    "market_watchdog": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 3000,
      "update_interval": 2.0,
      "providers": [
        "kalshi"
      ]
    },
    "trade_monitor": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 3000
    },
    "trade_manager": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 4000
    },
    "trade_executor": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 8001
    },
    "active_trade_supervisor": {
      "enabled": true,
      "host": "0.0.0.0",
      "port": 8007
    },
    "account_sync": {
      "enabled": true,
      "host": "0.0.0.0",
      "update_interval": 30.0,
      "providers": [
        "kalshi"
      ]
    }
  },
  "data": {
    "base_path": "backend/data",
    "databases": {
      "trades": "trade_history/trades.db",
      "prices": "price_history/",
      "accounts": "accounts/",
      "logs": "logs/"
    }
  },
  "providers": {
    "coinbase": {
      "base_url": "https://api.coinbase.com/v2",
      "timeout": 10
    },
    "kalshi": {
      "base_url": "https://api.elections.kalshi.com/trade-api/v2",
      "timeout": 10,
      "credentials_path": "backend/api/kalshi-api/kalshi-credentials"
    }
  },
  "indicators": {
    "momentum": {
      "enabled": true,
      "periods": [
        1,
        2,
        3,
        4,
        15,
        30
      ],
      "weights": [
        0.3,
        0.25,
        0.2,
        0.15,
        0.05,
        0.05
      ]
    }
  },
  "trading": {
    "default_position_size": 100,
    "max_position_size": 1000,
    "auto_stop_enabled": false,
    "account_mode": "demo"
  }
}